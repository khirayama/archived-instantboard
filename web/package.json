{
  "name": "instantboard-web-client",
  "version": "0.0.0",
  "description": "instantboard web client",
  "main": "src/index.js",
  "repository": {},
  "author": "khirayama",
  "license": "MIT",
  "engines": {
    "node": "^8.0.0",
    "npm": "^5.0.0"
  },
  "scripts": {
    "typings": "typings",
    "dev": "rm -rf dist && mkdir dist && cp src/index.html dist/index.html && concurrently 'browser-sync start --server ./dist --files dist/**/* --no-open --no-ui' 'npm run build:scripts:watch' 'npm run watch:styles'",
    "build:scripts": "webpack -p",
    "build:scripts:watch": "webpack -w",
    "build:styles": "postcss -c .postcssrc.js src/styles/index.css -o dist/index.css",
    "watch:styles": "postcss -c .postcssrc.js -w src/styles/index.css -o dist/index.css",
    "lint": "tslint src/**/*.ts",
    "lint:fix": "tslint --fix src/**/*.ts",
    "check": "npm-check-updates"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "browser-sync": "^2.18.12",
    "classnames": "^2.2.5",
    "concurrently": "^3.4.0",
    "cssnano": "^3.10.0",
    "npm-check-updates": "^2.11.3",
    "postcss-cli": "^4.0.0",
    "postcss-easy-import": "^2.1.0",
    "postcss-nested": "^2.0.2",
    "postcss-simple-vars": "^4.0.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-addons-transition-group": "^15.5.2",
    "react-dom": "^15.5.4",
    "ts-loader": "^2.1.0",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4",
    "typings": "^2.1.1",
    "webpack": "^2.6.1"
  }
}
